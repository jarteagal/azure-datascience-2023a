{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-upds-jal-dev-14m"
		},
		"Adls_linkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'Adls_linkedservice'"
		},
		"FileServer_linkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'FileServer_linkedservice'"
		},
		"Adls_linkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsupdsjaldevq95.dfs.core.windows.net/"
		},
		"FileServer_linkedservice_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\DESKTOP-2C1BGDJ\\D$\\Data"
		},
		"FileServer_linkedservice_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "MicrosoftAccount\\jarteag@gmail.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-upds-jal-dev-14m_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Adls_linkedservice_accountKey": {
						"value": "[parameters('Adls_linkedservice_accountKey')]"
					},
					"FileServer_linkedservice_password": {
						"value": "[parameters('FileServer_linkedservice_password')]"
					},
					"Adls_linkedservice_properties_typeProperties_url": {
						"value": "[parameters('Adls_linkedservice_properties_typeProperties_url')]"
					},
					"FileServer_linkedservice_properties_typeProperties_host": {
						"value": "[parameters('FileServer_linkedservice_properties_typeProperties_host')]"
					},
					"FileServer_linkedservice_properties_typeProperties_userId": {
						"value": "[parameters('FileServer_linkedservice_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}